# Global ignore for backend and frontend

# Environment files
.env.local
.env*.local
*.pem

# Python (Backend)
/backend/venv/  # Adjust path if venv is elsewhere
__pycache__/
*.pyc
*.pyo
*.pyd

# Node.js (Frontend & Backend)
/frontend/node_modules/
/backend/node_modules/  # Add if backend uses Node.js
/frontend/.pnp/
/frontend/.pnp.js
/frontend/.yarn/install-state.gz

# Build artifacts
/frontend/dist/
/frontend/build/
/frontend/.next/
/frontend/out/
/backend/build/  # Add if backend has build artifacts

# Logs
/frontend/npm-debug.log*
/frontend/yarn-debug.log*
/frontend/yarn-error.log*
/backend/*.log  # Add if backend generates logs

# Coverage reports
/frontend/coverage/
/backend/coverage/  # Add if backend has coverage reports

# OS-specific files
.DS_Store
Thumbs.db

# TypeScript
/frontend/*.tsbuildinfo
/frontend/next-env.d.ts

# Vercel deployment
/frontend/.vercel

# Misc
.idea/
.vscode/

# FAISS index and related files
backend/faiss/
backend/index.faiss
backend/index.pkl

# mobile frontend
mobile-frontend/node_modules/
mobile-frontend/android/
# Global log files
*.log